import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       double km_h, m_s;

       Scanner in = new Scanner(System.in);
       
       System.out.printIn("Digite a velocidade: ");
       km_h = in.nextDouble();

       km_h = m_s * 3.6
       System.out.printIn("Em km/h a velocidade Ã©: " + km_h );

    }
}
